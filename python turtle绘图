t.begin_fill()
t.showturtle()
t.right(90)
t.forward(70)
t.left(90)
t.forward(70)
t.left(45)
t.circle(25,180)
t.left(180)
t.circle(25,180)
t.end_fill()
t.hideturtle()
t.up()
t.goto(-186,-145)
t.down()
t.left(135)
t.color("red","red")
t.begin_fill()
t.showturtle()
t.right(90)
t.forward(70)
t.left(90)
t.forward(70)
t.left(45)
t.circle(25,180)
t.left(180)
t.circle(25,180)
t.end_fill()
t.pencolor("BLACK")
t.penup()
t.goto(80,180)
